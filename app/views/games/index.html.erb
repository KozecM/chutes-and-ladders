<h1>Games</h1>

<table>
  <thead>
    <tr>
      <td>Game Name</td>
      <td>Players</td>
    </tr>
  </thead>
  <tbody>
    <% @games.each do |game| %>
    
      <tr>
        <td><%= game.name %></td>
        <%game.players.each do |player| %>
          <td><%= player.name %></td>
        <% end %>
        <td></td>
      </tr>
    <% end %>
  </tbody>
</table>
